<manifest>
    <main-class>be.yurimoens.runemate.cfletcher.CFletcher</main-class>
    <name>CFletcher</name>
    <description>Fletches stuff.</description>
    <version>1.0.0</version>
    <compatibility>
        <game-type>RS3</game-type>
    </compatibility>
    <categories>
        <category>FLETCHING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>CFletcher</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Fletching</tag>
        <tag>Bows</tag>
        <tag>Stringing</tag>
    </tags>
</manifest>