<manifest>
    <main-class>be.yurimoens.runemate.cgranitecutter.CGraniteCutter</main-class>
    <name>CGraniteCutter</name>
    <description>Cuts granite.</description>
    <version>1.0.0</version>
    <compatibility>
        <game-type>RS3</game-type>
    </compatibility>
    <categories>
        <category>MINING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>CGraniteCutter</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Granite</tag>
    </tags>
</manifest>