<manifest>
    <main-class>be.yurimoens.runemate.cgraniteminer.CGraniteMiner</main-class>
    <name>CGraniteMiner</name>
    <description>Mines granite in the Al-Kharid Desert quarry. Can powermine, bank and use signs of the porter.</description>
    <version>1.0.0</version>
    <compatibility>
        <game-type>RS3</game-type>
    </compatibility>
    <categories>
        <category>MINING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>CGraniteMiner</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Mining</tag>
        <tag>Granite</tag>
        <tag>Powermining</tag>
    </tags>
</manifest>