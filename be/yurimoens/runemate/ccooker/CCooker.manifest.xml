<manifest>
    <main-class>be.yurimoens.runemate.ccooker.CCooker</main-class>
    <name>CCooker</name>
    <description>Cooks stuff.</description>
    <version>1.0.0</version>
    <compatibility>
        <game-type>RS3</game-type>
    </compatibility>
    <categories>
        <category>COOKING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>CCooker</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Cooking</tag>
        <tag>Portables</tag>
    </tags>
</manifest>